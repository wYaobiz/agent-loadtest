{
    "service": {
      "host": null,
      "port": 8081,
  
      "metrics": {
        "enabled": true,
        "collectDefaultMetrics": true,
        "requestDurationBuckets": [1, 5, 7, 9, 11, 13, 15, 20, 30]
      },
  
      "logging": {
        "level": "debug",
        "console": {
          "json": true,
          "colorize": false
        }
      }
    },
    "rendering": {
      "chromeBin": null,
      "args": [
        "--no-sandbox"
      ],
      "ignoresHttpsErrors": false,
  
      "timezone": null,
      "acceptLanguage": null,
      "width": 2000,
      "height": 1000,
      "deviceScaleFactor": 20,
      "maxWidth": 3080,
      "maxHeight": 3000,
      "maxDeviceScaleFactor": 20,
  
      "mode": "clustered",
      "clustering": {
        "mode": "browser",
        "maxConcurrency": 5,
        "timeout": 30
      },
  
      "verboseLogging": false,
      "dumpio": false
    }
  }